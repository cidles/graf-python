<graph xmlns="http://www.xces.org/ns/GrAF/1.0/">
  <graphHeader>
    <labelsDecl/>
    <dependencies>
      <dependsOn f.id="utterance"/>
    </dependencies>
    <annotationSpaces>
      <annotationSpace as.id="translation"/>
    </annotationSpaces>
  </graphHeader>
  <a as="translation" label="translation" ref="utterance-n0" xml:id="translation-a0">
    <fs>
      <f name="translation">They say:</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n1" xml:id="translation-a1">
    <fs>
      <f name="translation">that there was a king.</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n2" xml:id="translation-a2">
    <fs>
      <f name="translation">This king had a light post in the centre of his town</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n3" xml:id="translation-a3">
    <fs>
      <f name="translation">and whatever phone wires there were,</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n4" xml:id="translation-a4">
    <fs>
      <f name="translation">they were connected to this (light post)</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n5" xml:id="translation-a5">
    <fs>
      <f name="translation">anyone who had a petition,</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n6" xml:id="translation-a6">
    <fs>
      <f name="translation">when he shook this (light post),</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n7" xml:id="translation-a7">
    <fs>
      <f name="translation">the king was informed there (in his palace).</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n8" xml:id="translation-a8">
    <fs>
      <f name="translation">One day he noticed</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n9" xml:id="translation-a9">
    <fs>
      <f name="translation">that a dragon had come and</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n10" xml:id="translation-a10">
    <fs>
      <f name="translation">was shaking this (light post).</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n11" xml:id="translation-a11">
    <fs>
      <f name="translation">The king sent a person</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n12" xml:id="translation-a12">
    <fs>
      <f name="translation">(to check) what this was.</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n13" xml:id="translation-a13">
    <fs>
      <f name="translation">They (the ones whom the king had sent) came (back),</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n14" xml:id="translation-a14">
    <fs>
      <f name="translation">he (one of them) said:</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n15" xml:id="translation-a15">
    <fs>
      <f name="translation">Lord king, it is a dragon.</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n16" xml:id="translation-a16">
    <fs>
      <f name="translation">He (the king) said:</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n17" xml:id="translation-a17">
    <fs>
      <f name="translation">Oh… this dragon has something to say,</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n18" xml:id="translation-a18">
    <fs>
      <f name="translation">it has a petition,</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n19" xml:id="translation-a19">
    <fs>
      <f name="translation">who can bring its petition to me,</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n20" xml:id="translation-a20">
    <fs>
      <f name="translation">as for me, I don't know</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n21" xml:id="translation-a21">
    <fs>
      <f name="translation">it cannot talk.</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n22" xml:id="translation-a22">
    <fs>
      <f name="translation">A carpenter said:</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n23" xml:id="translation-a23">
    <fs>
      <f name="translation">I know (what the dragon wants).</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n24" xml:id="translation-a24">
    <fs>
      <f name="translation">He (the king) said:</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n25" xml:id="translation-a25">
    <fs>
      <f name="translation">(If) you know</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n26" xml:id="translation-a26">
    <fs>
      <f name="translation">(and if) you bring its petition,</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n27" xml:id="translation-a27">
    <fs>
      <f name="translation">I will give you this much reward.</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n28" xml:id="translation-a28">
    <fs>
      <f name="translation">He (the carpenter) said:</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n29" xml:id="translation-a29">
    <fs>
      <f name="translation">Very well.</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n30" xml:id="translation-a30">
    <fs>
      <f name="translation">When the carpenter came with blades and saws,</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n31" xml:id="translation-a31">
    <fs>
      <f name="translation">the dragon released (the light post) and</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n32" xml:id="translation-a32">
    <fs>
      <f name="translation">set out and</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n33" xml:id="translation-a33">
    <fs>
      <f name="translation">the dragon gave the carpenter one hint after another and</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n34" xml:id="translation-a34">
    <fs>
      <f name="translation">went into the mountain.</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n35" xml:id="translation-a35">
    <fs>
      <f name="translation">When he went there,</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n36" xml:id="translation-a36">
    <fs>
      <f name="translation">good heavens ohhh ... this dragon has a mother as well.</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n37" xml:id="translation-a37">
    <fs>
      <f name="translation">Its mother, there are this kind of wild mountain goats with big horns,</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n38" xml:id="translation-a38">
    <fs>
      <f name="translation">it has caught (one) of them and</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n39" xml:id="translation-a39">
    <fs>
      <f name="translation">its horns have got stuck in its (the mother’s) throat and</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n40" xml:id="translation-a40">
    <fs>
      <f name="translation">it is short of breath,</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n41" xml:id="translation-a41">
    <fs>
      <f name="translation">it (the dragon) has come here</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n42" xml:id="translation-a42">
    <fs>
      <f name="translation">(and) it has informed the king.</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n43" xml:id="translation-a43">
    <fs>
      <f name="translation">It (the dragon) gave a hint and</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n44" xml:id="translation-a44">
    <fs>
      <f name="translation">he (the carpenter) sawed the wild goat's horns and</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n45" xml:id="translation-a45">
    <fs>
      <f name="translation">it was rescued,</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n46" xml:id="translation-a46">
    <fs>
      <f name="translation">(and) became free.</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n47" xml:id="translation-a47">
    <fs>
      <f name="translation">This dragon went to another place and</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n48" xml:id="translation-a48">
    <fs>
      <f name="translation">it gave him (the carpenter) a big rewards, jewelry.</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n49" xml:id="translation-a49">
    <fs>
      <f name="translation">Then when he (the carpenter) went a little</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n50" xml:id="translation-a50">
    <fs>
      <f name="translation">again it (the dragon) gave him a hint.</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n51" xml:id="translation-a51">
    <fs>
      <f name="translation">He (the carpenter) stopped.</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n52" xml:id="translation-a52">
    <fs>
      <f name="translation">It (the dragon) took a seed of such a size and</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n53" xml:id="translation-a53">
    <fs>
      <f name="translation">from its mouth threw it among the jewels, inside his bag.</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n54" xml:id="translation-a54">
    <fs>
      <f name="translation">When he came (back to the king),</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n55" xml:id="translation-a55">
    <fs>
      <f name="translation">the king said:</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n56" xml:id="translation-a56">
    <fs>
      <f name="translation">What petition did it (the dragon) have?</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n57" xml:id="translation-a57">
    <fs>
      <f name="translation">He (the carpenter) said:</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n58" xml:id="translation-a58">
    <fs>
      <f name="translation">Actually, its mother was strangled in this way and</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n59" xml:id="translation-a59">
    <fs>
      <f name="translation">it (the dragon) came and</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n60" xml:id="translation-a60">
    <fs>
      <f name="translation">I freed (and I rescued it) and</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n61" xml:id="translation-a61">
    <fs>
      <f name="translation">then it gave me this much jewelry and</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n62" xml:id="translation-a62">
    <fs>
      <f name="translation">this very seed, it also gave me this,</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n63" xml:id="translation-a63">
    <fs>
      <f name="translation">it threw it from its mouth.</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n64" xml:id="translation-a64">
    <fs>
      <f name="translation">He (the king) said:</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n65" xml:id="translation-a65">
    <fs>
      <f name="translation">Well, this is certainly jewelry,</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n66" xml:id="translation-a66">
    <fs>
      <f name="translation">(but) we don't know this.</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n67" xml:id="translation-a67">
    <fs>
      <f name="translation">They (the king and his consultants) consulted,</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n68" xml:id="translation-a68">
    <fs>
      <f name="translation">(and) they said:</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n69" xml:id="translation-a69">
    <fs>
      <f name="translation">Let's sow this.</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n70" xml:id="translation-a70">
    <fs>
      <f name="translation">They sowed it in a good place, in a garden, in a plot and in (fertile) and soft soil and</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n71" xml:id="translation-a71">
    <fs>
      <f name="translation">a melon bush grew up and</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n72" xml:id="translation-a72">
    <fs>
      <f name="translation">it gave so many melons.</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n73" xml:id="translation-a73">
    <fs>
      <f name="translation">Nobody ate these melons out of fear</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n74" xml:id="translation-a74">
    <fs>
      <f name="translation">since they were from the mouth of the dragon,</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n75" xml:id="translation-a75">
    <fs>
      <f name="translation">they were snake's poison.</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n76" xml:id="translation-a76">
    <fs>
      <f name="translation">They (the melons) produce so many seeds and</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n77" xml:id="translation-a77">
    <fs>
      <f name="translation">they (the king’s servants) went (and told the king),</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n78" xml:id="translation-a78">
    <fs>
      <f name="translation">the king sowed a large melon-bed and</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n79" xml:id="translation-a79">
    <fs>
      <f name="translation">made an old man the gardener.</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n80" xml:id="translation-a80">
    <fs>
      <f name="translation">It began, this melon-bed, to grow melons and</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n81" xml:id="translation-a81">
    <fs>
      <f name="translation">produced a lot of melons.</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n82" xml:id="translation-a82">
    <fs>
      <f name="translation">The old man didn't do, you know, at first,</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n83" xml:id="translation-a83">
    <fs>
      <f name="translation">he didn't trust and</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n84" xml:id="translation-a84">
    <fs>
      <f name="translation">and he had a little donkey,</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n85" xml:id="translation-a85">
    <fs>
      <f name="translation">first, he cut these very melons to pieces in front of the donkey and</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n86" xml:id="translation-a86">
    <fs>
      <f name="translation">gave (them to the donkey).</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n87" xml:id="translation-a87">
    <fs>
      <f name="translation">So this donkey became strong.</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n88" xml:id="translation-a88">
    <fs>
      <f name="translation">The old man began to eat,</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n89" xml:id="translation-a89">
    <fs>
      <f name="translation">Wow, what (good) things these are.</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n90" xml:id="translation-a90">
    <fs>
      <f name="translation">The old man became strong as well.</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n91" xml:id="translation-a91">
    <fs>
      <f name="translation">One day when the king came,</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n92" xml:id="translation-a92">
    <fs>
      <f name="translation">he (the king) said:</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n93" xml:id="translation-a93">
    <fs>
      <f name="translation">(Dear) old man what are these?</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n94" xml:id="translation-a94">
    <fs>
      <f name="translation">He (the old man) said:</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n95" xml:id="translation-a95">
    <fs>
      <f name="translation">These are very good things,</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n96" xml:id="translation-a96">
    <fs>
      <f name="translation">I have also eaten (from them),</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n97" xml:id="translation-a97">
    <fs>
      <f name="translation">I have also given (them) to this donkey and</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n98" xml:id="translation-a98">
    <fs>
      <f name="translation">you, too, (should) eat (it).</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n99" xml:id="translation-a99">
    <fs>
      <f name="translation">The vizier said:</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n100" xml:id="translation-a100">
    <fs>
      <f name="translation">No, first I will eat,</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n101" xml:id="translation-a101">
    <fs>
      <f name="translation">don't eat, oh king!</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n102" xml:id="translation-a102">
    <fs>
      <f name="translation">The vizier ate,</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n103" xml:id="translation-a103">
    <fs>
      <f name="translation">wow what tasty things are these.</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n104" xml:id="translation-a104">
    <fs>
      <f name="translation">They ate and</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n105" xml:id="translation-a105">
    <fs>
      <f name="translation">when they had eaten,</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n106" xml:id="translation-a106">
    <fs>
      <f name="translation">now they said:</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n107" xml:id="translation-a107">
    <fs>
      <f name="translation">Let’s give it a name.</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n108" xml:id="translation-a108">
    <fs>
      <f name="translation">Then they named the melon 'xarmizza' (which means donkey-taste),</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n109" xml:id="translation-a109">
    <fs>
      <f name="translation">because for the first time it was the donkey that tasted it.</f>
    </fs>
  </a>
  <a as="translation" label="translation" ref="utterance-n110" xml:id="translation-a110">
    <fs>
      <f name="translation">(the name) xarmizza remained from there</f>
    </fs>
  </a>
</graph>
